extends ./layout/_layout

block title
    title Una's Portfolio
    link(rel="stylesheet", href="css/all.css?"+new Date().getTime())
block header
    header.header.header-index.min-vh-100
        nav.navbar.navbar-header.container-fluid
            a.navbar-brand.brand
            button.navbar-toggler.navbar-toggler-x(type="button",data-toggle="collapse",data-target="#headNavbarContent",aria-controls="headNavbarContent",aria-expanded="false",aria-label="Toggle navigation")
                .toggler-bar
        .collapse.collapse-header#headNavbarContent
            .collapse-content.container-fluid
                a.navbar-brand.brand-nav
                .collapse-title
                    | UnaXie’s
                    br
                    | Portfolio
                ul.navbar-nav
                    li.nav-item(attr-operate="navbar-sm")
                        a.nav-link(href="#",data-target="#HOME",data-operate="SmoothScroll")
                            span HOME
                    li.nav-item(attr-operate="navbar-sm")
                        a.nav-link(href="#",data-target="#ABOUT",data-operate="SmoothScroll") 
                            span ABOUT
                    li.nav-item(attr-operate="navbar-sm")
                        a.nav-link(href="#",data-target="#WORKS",data-operate="SmoothScroll")
                            span WORKS
                .navbar-underNav   
                    a.btn.btn-circle(href="#")
                        i.icon.icon-mail
                    a.btn.btn-circle(href="#")
                        i.icon.icon-linkedin
                    a.disabled.btn.btn-circle(href="#")
                        i.icon.icon-github
        .mainArea.container-lg
            .mainPic
            .mainTitle
        .btn.btn-mouse

